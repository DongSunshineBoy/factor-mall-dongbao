<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dwt.mapper.PProductDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dwt.entity.PProductEntity">
        <result column="id" property="id" />
        <result column="p_product_name" property="pProductName" />
        <result column="p_number" property="pNumber" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        p_product_name, p_number
    </sql>

</mapper>
